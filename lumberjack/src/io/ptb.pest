tree = _{SOI ~ node ~ EOI}
node = _{ preterminal | nonterminal }
nonterminal = { open ~ node_label ~ node+ ~ close }
char = _{ !( WHITESPACE | "(" | ")" ) ~ ANY }
preterminal = { open ~ node_label ~ terminal ~ close }
terminal = @{ char+ }
node_label = @{ char+ }
open = _{ "(" }
close = _{ ")" }
WHITESPACE = _{ " " | NEWLINE }